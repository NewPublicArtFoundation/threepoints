<%= simple_form_for(@graffiti) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.association :location %>
    <%= f.input :tag_list %>
    <%= f.input :discovered_at, prompt: "Unknown" %>
    <%= f.input :painted_at, prompt: "Unknown" %>
    <%= f.input :buffed_at, prompt: "Not Applicable" %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
