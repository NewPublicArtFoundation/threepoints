<%= simple_form_for(@graffiti_picture) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :graffiti %>
    <%= f.input :image_url %>
    <%= f.input :image_description %>
    <%= f.association :location %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
