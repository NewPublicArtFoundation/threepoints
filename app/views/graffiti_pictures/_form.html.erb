<%= simple_form_for(@graffiti_picture, url: graffiti_pictures_path, html: {multipart: true}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :graffiti %>
    <%= f.input :uploaded_image %>
    <%= f.input :image_url %>
    <%= f.input :image_description %>
    <%= f.input :tag_list %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
