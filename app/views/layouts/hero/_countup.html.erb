<h1 class="text-center hero-white">Over <strong id="count-amount"><%= InstagramArt.count/2 %></strong> pieces</h1>

<script src="/js/countup.js"></script>

<script>
  var options = {
    useEasing : true,
    useGrouping : true,
    separator : ',',
    decimal : '.',
    prefix : '',
    suffix : ''
  }
  var countUp = new countUp("count-amount", <%= InstagramArt.count/2 %>, 20000, 0, 4.5, options);
  countUp.start();
</script>
