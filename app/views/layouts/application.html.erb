<!DOCTYPE html>
<html>
<head>
  <title>Threepoints</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "vendor/modernizr" %>

  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
<!-- Prelang: Analytics / Google Analytics -->
    <%= analytics_init if Rails.env.production? %></head>
    <%= render 'layouts/tracking/mixpanel' %>
    <%= render 'layouts/tracking/googleanalytics' %>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/Chart.js/0.2.0/Chart.min.js"></script>
  </head>

  <body>



    <%= render 'layouts/navigation/topnav' %>
      <div class="row alert-container">
        <% flash.each do |name, msg| %>
        <div data-alert class="alert-box info radius">
            <%= content_tag :div, msg, :id => "flash_#{name}" %>
          <a href="#" class="close">&times;</a>
        </div>
        <% end %>
      </div>


    <%= yield %>



    <%= render 'layouts/footer/footerCase' %>

    <%= javascript_include_tag "application" %>

</body>
</html>
