.social-link{
  &:before{
    content: '';
    height: 15px;
    width: 15px;
    background: image-url('checksuccess.png');
    display: inline-block;
    background-size: contain;
    margin-right:4px;
  }
}

.owl-carousel-profile{
  position: relative;
  .owl-controls{
    width:100%;
    position: absolute;
    top: 50%;
    .owl-prev{
      float: left;
    }
    .owl-next{
      float: right;
    }
    .owl-dots{
      margin-top: 50%;
    }
  }
  .owl-stage{
    max-height: 380px;
    overflow: hidden;
  }
}
